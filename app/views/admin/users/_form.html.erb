<%= form_for [:admin, @user], url: admin_users_path, :method => :post do |f| %>
   <%= f.label :username %><br>
   <%= f.text_field :username, class: "form-control", placeholder: "username" %><br>
   <%= f.label :year %><br>
   <%= f.number_field :year, class: "form-control", placeholder: "joinyear" %><br>
   <%= f.label :email %><br>
   <%= f.text_field :email, class: "form-control", placeholder: "email" %><br>
   <%= f.label :password %><br>
   <%= f.password_field :password, class: "form-control", placeholder: "password" %><br>
   <%= f.label :labo %><br>
   <%= f.collection_select :labo, Labo.all, :id, :name %>
   <%= f.submit_tag "submit", class: "btn btn-lg btn-info btn-block" %>
<% end %>
